algoritmo "Alistamento Militar"
var
    idade: inteiro
    altura: real
inicio
    escreva("Digite a sua idade: ")
    leia(idade)
limpaTela()

        escreva("Digite a sua altura em metros: ")
    leia(altura)
limpaTela()

Se(idade>=18)e(altura>=1.70)Entao
Escreva(“Você pode se alistar.”)
Senao
Escreva(“Você não pode se alistar.”)
FimSe
FIMALGORITMO
